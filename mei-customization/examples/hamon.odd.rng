            <ref name="instrGrp"/>
         </optional>
         <optional>
            <ref name="model.staffGrpLike"/>
         </optional>
         <zeroOrMore>
            <ref name="grpSym"/>
         </zeroOrMore>
         <optional>
            <ref name="ambitus"/>
         </optional>
         <ref name="att.common.attributes"/>
         <ref name="att.scoreDef.log.attributes"/>
