            <ref name="data.DURQUALITY.mensural"/>
         </attribute>
      </optional>
   </define>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="mei-att.duration.quality-check_duplex_quality-constraint-rule-124">
