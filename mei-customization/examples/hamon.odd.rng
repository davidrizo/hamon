      </element>
   </define>
   <define name="monogr">
      <element name="monogr">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(monograph level) Contains bibliographic elements describing an item, for example, a published book or journal, score, recording, or an unpublished manuscript.</a:documentation>
         <group>
            <optional>
               <choice>
                  <group>
                     <choice>
                        <ref name="model.respLikePart"/>
                        <ref name="respStmt"/>
                     </choice>
                     <zeroOrMore>
                        <choice>
                           <ref name="model.respLikePart"/>
                           <ref name="respStmt"/>
                        </choice>
                     </zeroOrMore>
                     <oneOrMore>
                        <ref name="model.titleLike"/>
                     </oneOrMore>
                     <zeroOrMore>
                        <choice>
                           <ref name="editor"/>
                           <ref name="model.identifierLike"/>
                           <ref name="respStmt"/>
                        </choice>
                     </zeroOrMore>
                  </group>
                  <group>
                     <oneOrMore>
                        <ref name="model.titleLike"/>
                     </oneOrMore>
                     <zeroOrMore>
                        <choice>
                           <ref name="model.respLikePart"/>
                           <ref name="model.identifierLike"/>
                           <ref name="respStmt"/>
                        </choice>
                     </zeroOrMore>
                  </group>
               </choice>
            </optional>
            <zeroOrMore>
               <choice>
                  <ref name="model.annotLike"/>
                  <ref name="corpName"/>
               </choice>
            </zeroOrMore>
            <zeroOrMore>
               <ref name="model.editionLike"/>
               <zeroOrMore>
                  <choice>
                     <ref name="model.identifierLike"/>
                     <ref name="editor"/>
                     <ref name="respStmt"/>
                  </choice>
               </zeroOrMore>
            </zeroOrMore>
            <ref name="imprint"/>
            <zeroOrMore>
               <choice>
                  <ref name="imprint"/>
                  <ref name="extent"/>
                  <ref name="biblScope"/>
               </choice>
            </zeroOrMore>
         </group>
         <ref name="att.common.attributes"/>
         <ref name="att.bibl.attributes"/>
         <ref name="att.dataPointing.attributes"/>
         <ref name="att.pointing.attributes"/>
         <ref name="att.recordType.attributes"/>
         <ref name="att.targetEval.attributes"/>
