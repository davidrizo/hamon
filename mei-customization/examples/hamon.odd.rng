         <ref name="att.responsibility.attributes"/>
         <attribute name="type">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Characterizes the property name.</a:documentation>
            <choice>
               <value>unicode</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">A registered Unicode normative or informative property name.</a:documentation>
               <value>local</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">A locally defined name.</a:documentation>
            </choice>
         </attribute>
         <empty/>
      </element>
   </define>
   <define name="propValue">
      <element name="propValue">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(property value) A single property value.</a:documentation>
         <text/>
         <ref name="att.common.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="symbolDef">
      <element name="symbolDef">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(symbol definition) Declaration of an individual symbol in a symbolTable.</a:documentation>
         <optional>
            <ref name="symName"/>
         </optional>
         <zeroOrMore>
            <ref name="symProp"/>
         </zeroOrMore>
         <zeroOrMore>
            <ref name="mapping"/>
         </zeroOrMore>
         <choice>
            <zeroOrMore>
               <choice>
                  <ref name="model.graphicPrimitiveLike"/>
                  <ref name="symbol"/>
                  <ref name="graphic"/>
               </choice>
            </zeroOrMore>
         </choice>
         <zeroOrMore>
            <ref name="model.annotLike"/>
         </zeroOrMore>
         <ref name="att.common.attributes"/>
         <ref name="att.coordinated.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="symName">
      <element name="symName">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(symbol name) Contains the name of a symbol, expressed following Unicode conventions.</a:documentation>
         <text/>
         <ref name="att.common.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="symProp">
      <element name="symProp">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(symbol property) Provides a name and value for some property of the parent symbol.</a:documentation>
         <ref name="propName"/>
         <ref name="propValue"/>
         <ref name="att.common.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="symbolTable">
      <element name="symbolTable">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Contains a set of user-defined symbols.</a:documentation>
         <oneOrMore>
            <ref name="symbolDef"/>
         </oneOrMore>
         <ref name="att.common.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="att.harm.mxml.attributes">
      <ref name="att.harm.mxml.attribute.root.pname"/>
      <ref name="att.harm.mxml.attribute.root.accid"/>
      <ref name="att.harm.mxml.attribute.root.oct"/>
      <ref name="att.harm.mxml.attribute.bass.pname"/>
      <ref name="att.harm.mxml.attribute.bass.accid"/>
      <ref name="att.harm.mxml.attribute.bass.oct"/>
      <ref name="att.harm.mxml.attribute.chord.kind"/>
      <ref name="att.harm.mxml.attribute.chord.text"/>
   </define>
   <define name="att.harm.mxml.attribute.root.pname">
      <optional>
         <attribute name="root.pname">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Pitch name of the chord root (c–b). </a:documentation>
            <data type="token">
               <param name="pattern">[a-g]</param>
            </data>
         </attribute>
      </optional>
   </define>
   <define name="att.harm.mxml.attribute.root.accid">
      <optional>
         <attribute name="root.accid">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Accidental of the chord root (s, f, n, etc.). </a:documentation>
            <choice>
               <ref name="data.ACCIDENTAL.WRITTEN.basic"/>
               <ref name="data.ACCIDENTAL.WRITTEN.extended"/>
               <ref name="data.ACCIDENTAL.aeu"/>
               <ref name="data.ACCIDENTAL.persian"/>
            </choice>
         </attribute>
      </optional>
   </define>
   <define name="att.harm.mxml.attribute.root.oct">
      <optional>
         <attribute name="root.oct">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Octave number of the chord root, if needed. </a:documentation>
            <data type="nonNegativeInteger">
               <param name="maxInclusive">9</param>
            </data>
         </attribute>
      </optional>
   </define>
   <define name="att.harm.mxml.attribute.bass.pname">
      <optional>
         <attribute name="bass.pname">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Pitch name of the bass note (c–b). </a:documentation>
            <data type="token">
               <param name="pattern">[a-g]</param>
            </data>
         </attribute>
      </optional>
   </define>
   <define name="att.harm.mxml.attribute.bass.accid">
      <optional>
         <attribute name="bass.accid">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Accidental of the bass note. </a:documentation>
            <choice>
               <ref name="data.ACCIDENTAL.WRITTEN.basic"/>
               <ref name="data.ACCIDENTAL.WRITTEN.extended"/>
               <ref name="data.ACCIDENTAL.aeu"/>
               <ref name="data.ACCIDENTAL.persian"/>
            </choice>
         </attribute>
      </optional>
   </define>
   <define name="att.harm.mxml.attribute.bass.oct">
      <optional>
         <attribute name="bass.oct">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Octave number of the bass note, if needed. </a:documentation>
            <data type="nonNegativeInteger">
               <param name="maxInclusive">9</param>
            </data>
         </attribute>
      </optional>
   </define>
   <define name="att.harm.mxml.attribute.chord.kind">
      <optional>
         <attribute name="chord.kind">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">MusicXML-style chord kind. </a:documentation>
            <choice>
               <value>major</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <value>minor</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <value>augmented</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <value>diminished</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <value>dominant</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <value>major-seventh</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <value>minor-seventh</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <value>half-diminished</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <value>power</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            </choice>
         </attribute>
      </optional>
   </define>
   <define name="att.harm.mxml.attribute.chord.text">
      <optional>
         <attribute name="chord.text">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Normalized chord symbol text, if different from the element content. </a:documentation>
            <data type="token">
               <param name="pattern">(\p{L}|\p{N}|\p{P}|\p{S})*</param>
            </data>
         </attribute>
      </optional>
   </define>
   <start>
      <choice>
         <ref name="mei"/>
         <ref name="meiHead"/>
         <ref name="meiCorpus"/>
         <ref name="music"/>
      </choice>
   </start> 
   <sch:ns xmlns="http://www.tei-c.org/ns/1.0"
           xmlns:rng="http://relaxng.org/ns/structure/1.0"
           xmlns:sch="http://purl.oclc.org/dsdl/schematron"
           xmlns:xi="http://www.w3.org/2001/XInclude"
           prefix="mei"
           uri="http://www.music-encoding.org/ns/mei"/> 
   <sch:pattern xmlns="http://www.tei-c.org/ns/1.0"
                xmlns:rng="http://relaxng.org/ns/structure/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns:xi="http://www.w3.org/2001/XInclude">
              <sch:rule context="mei:harm">
                <sch:assert test="not(@root.pname) or @chord.kind"> If @root.pname is present,
                  @chord.kind should also be given. </sch:assert>
              </sch:rule>
            </sch:pattern>
</grammar>
