            <attribute name="context">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Circumstances in which the element appears, an XPath expression.</a:documentation>
               <data type="string"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="occurs">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Number of occurrences in the defined context.</a:documentation>
               <data type="nonNegativeInteger"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="withid">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Number of occurrences in the defined context that have an <code xmlns="http://www.w3.org/1999/xhtml"
